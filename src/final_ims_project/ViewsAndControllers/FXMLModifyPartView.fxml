                  </HBox>
                  <HBox prefHeight="35.0" prefWidth="380.0">
                     <children>
                        <Label prefWidth="100.0" text="Name" />
                        <TextField fx:id="nameTextfield" promptText="name" />
                     </children>
                  </HBox>
