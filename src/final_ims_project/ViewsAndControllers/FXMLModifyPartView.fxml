<AnchorPane id="AnchorPane" prefHeight="375.0" prefWidth="298.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="final_ims_project.ViewsAndControllers.FXMLModifyPartViewController">
     <children>
